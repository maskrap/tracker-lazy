<%= form_for [@user, @binge] do |f| %>
 <%= f.label :show %>
 <%= f.text_field :show %>
 <%= link_to 'Add a New Show', new_show_path, id: "new_show_link", remote: true %>
 <%= f.label :start %>
 <%= f.datetime_field :start%>
 <%= f.label :finish %>
 <%= f.datetime_field :finish %>
 <%= f.submit "Log Binge Session" %>
<%end%>
